{
  "name": "twitch-multi-player-reactive",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "generate:schema:twitch": "npx openapi-typescript oas/twitch.yml --output src/lib/types/schema-twitch.ts",
    "generate:schema:mine": "npx openapi-typescript ../oas/mine.yml --output src/lib/types/schema-mine.ts",
    "generate:schema": "npm run generate:schema:twitch ; npm run generate:schema:mine",
    "serve:bff": "CALLBACK_ENDPOINT=$(curl -s localhost:4040/api/tunnels | jq -r '.tunnels[].public_url' | grep -e 'https.*') ts-node -r tsconfig-paths/register src/bff/index.ts",
    "serve:callback": "ts-node -r tsconfig-paths/register src/callback/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vue/cli": "^4.5.12",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pusher": "^5.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4",
    "uninstall": "0.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/express": "^4.17.11",
    "openapi-typescript": "^3.2.4",
    "prettier": "^2.2.1",
    "ts-loader": "^9.1.2",
    "webpack": "^5.37.0",
    "webpack-cli": "^4.7.0"
  }
